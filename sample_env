# Sample environment variables for our project.
#
# Installation:
#
#   cp ./sample_env ./env.local
#   # make modifications to the environment
#   source ./env.local
#
# Check the current environment:
#
#   env
#
export PQ_CONN="user=hogbait dbname=hurricanedev host=/var/run/postgresql/ sslmode=disable"
export PQ_CONN_TEST="user=hogbait dbname=hurricanetest host=/var/run/postgresql/ sslmode=disable"
export GOPATH=$PWD
